{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"semi": true,
				"bracketSameLine": true,
				"htmlWhitespaceSensitivity": "ignore"
			}
		}
	]
}
